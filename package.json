{
  "name": "docker-logentries",
  "version": "1.0.1",
  "description": "Forward all logs from all running docker containers to Logentries",
  "repository": {
    "type": "git",
    "url": "https://github.com/octoblu/docker-logentries.git"
  },
  "main": "command.js",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "coverage": "nyc npm test",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "author": "Matteo Collina <matteo.collina@nearform.com>",
  "license": "MIT",
  "dependencies": {
    "docker-allcontainers": "^0.6.1",
    "docker-event-log": "^0.1.2",
    "docker-loghose": "^1.1.0",
    "docker-stats": "^0.7.0",
    "dockerode": "^2.2.2",
    "end-of-stream": "^1.1.0",
    "envalid": "^2.4.1",
    "lodash": "^4.17.2",
    "minimist": "^1.1.1",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "standard": "^8.6.0",
    "nyc": "^8.3.0",
    "mocha-osx-reporter": "^0.1.2",
    "mocha-multi": "^0.9.1",
    "mocha": "^2.5.3"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  }
}
